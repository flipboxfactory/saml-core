{
    "name": "flipboxfactory/saml-core",
    "description": "SAML SSO Core",
    "version": "4.2.3",
    "type": "library",
    "keywords": [
        "cms",
        "craftcms",
        "saml",
        "service provider",
        "federated authentication",
        "sso",
        "yii2"
    ],
    "require": {
        "craftcms/cms": "^4.0",
        "flipboxfactory/saml-core-psr-log": "^1.0 || ^3.0",
        "simplesamlphp/saml2": "^4.6.14",
        "flipboxfactory/keychain": "^4.0.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "flipbox\\saml\\core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "flipbox\\saml\\core\\tests\\": "tests/src/"
        }
    },
    "support": {
        "email": "support@flipboxfactory.com",
        "issues": "https://github.com/flipboxfactory/saml-core/issues?state=open",
        "source": "https://github.com/flipboxfactory/saml-core",
        "docs": "https://github.com/flipboxfactory/saml-core",
        "rss": "https://github.com/flipboxfactory/saml-core/commits/master.atom"
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        }
    }
}
